<h3>Your Quotes</h3>
<hr />
<% @quotes.each do |quote| %>
    <div class="card bg-body-tertiary" style="margin-bottom:10px">
    <div class="card-body">
        <h3 class="card-title"><%= quote.quote_text %></h3>
        <p class="card-text"><%= quote.philosophers.fname %><br />[<a href="/quotes/<%=quote.philosopher_id %>"><%= quote.philosopher_id %></a>]</p>
    </div>
    </div>
<% end %>